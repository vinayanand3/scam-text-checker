{
    "manifest_version": 3,
    "name": "Scam Text Checker",
    "version": "1.1",
    "description": "Analyzes a piece of text to determine if it's likely a scam.",
    "permissions": [],
    "host_permissions": [
      "https://*.run.app" 
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  
